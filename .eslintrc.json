{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "max-len": "off",
        "no-underscore-dangle": "off",
        "no-unused-expressions": "off",
        "no-console": "off",
        "camelcase": "off",
        "no-new": "off"
    }
}